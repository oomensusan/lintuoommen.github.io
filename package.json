{
  "name": "lintu-portfolio",
  "version": "1.0.0",
  "homepage": "https://lintuoommen.github.io",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run copy-to-docs",
    "copy-to-docs": "rm -rf docs/* && cp -r build/* docs/",
    "build-windows": "react-scripts build && rmdir /s docs && xcopy build docs /e /i"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  }
}
